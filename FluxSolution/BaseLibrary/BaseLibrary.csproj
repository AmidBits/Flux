<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
		<Nullable>enable</Nullable>
		<Authors>Robert Hugo</Authors>
		<Company></Company>
		<PackageId>BaseLibrary</PackageId>
		<Product>Flux</Product>
		<Description>Flux is a free .NET extension API with various functionalities.</Description>
		<Copyright></Copyright>
		<PackageProjectUrl>https://github.com/AmidBits/Flux</PackageProjectUrl>
		<RepositoryUrl>https://github.com/AmidBits/Flux</RepositoryUrl>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <None Update="Resources\*\*.*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Source\Text\Markup\" />
  </ItemGroup>

</Project>
